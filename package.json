{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "5.2.0"
  },
  "scripts": {
    "start": "hexo server --draft --port",
    "clean": "hexo clean",
    "build": "NODE_ENV=production hexo generate --concurrency 3",
    "deploy": "hexo deploy",
    "new": "hexo new",
    "minify": "gulp",
    "b:d": "yarn clean && yarn build && yarn minify && yarn deploy",
    "st:add": "git subtree add --prefix=themes/spa git@github.com:daief/hexo-theme-spa.git main --squash",
    "st:pull": "git subtree pull --prefix=themes/spa git@github.com:daief/hexo-theme-spa.git master --squash",
    "st:split": "git subtree split --rejoin --prefix=themes/spa HEAD",
    "st:push": "git subtree push --prefix=themes/spa git@github.com:daief/hexo-theme-spa.git master"
  },
  "saveDeps": {
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-baidu-sitemap": "^0.1.2",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^2.0.0",
    "hexo-generator-index": "^1.0.0",
    "hexo-generator-search": "^2.3.0",
    "hexo-generator-searchdb": "^1.0.8",
    "hexo-generator-sitemap": "^1.2.0",
    "hexo-generator-tag": "^1.0.0"
  },
  "dependencies": {
    "cross-env": "^6.0.0",
    "hexo": "~5.2.0",
    "hexo-deployer-git": "^0.3.1",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^3.3.0",
    "hexo-renderer-stylus": "^1.0.0",
    "hexo-server": "^2.0.0",
    "hexo-wordcount": "^6.0.1",
    "lodash": "^4.17.20",
    "prismjs": "^1.19.0"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-terser": "^1.2.0",
    "prettier": "^2.0.5"
  }
}
